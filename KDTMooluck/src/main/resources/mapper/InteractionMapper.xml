<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.kdt.mooluck.interaction.mapper.InteractionMapper">
    <select id="getInteractionList" parameterType="int" resultType="org.kdt.mooluck.interaction.dto.InteractionDTO">
        select *
        from interaction
        where elder_id = #{elderId}
    </select>
</mapper>