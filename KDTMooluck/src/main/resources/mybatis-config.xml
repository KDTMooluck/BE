    <typeAliases>
        <package name="org.kdt.mooluck.admin.dto" />
        <package name="org.kdt.mooluck.interaction.dto" />
    </typeAliases>

    <mappers>
        <mapper resource="mapper/AdminMapper.xml" />
        <mapper resource="mapper/InteractionMapper.xml" />
    </mappers>
</configuration>